{
  "name": "groq-rag",
  "version": "0.1.0",
  "description": "Extended Groq SDK with RAG, web browsing, and agent capabilities",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/ tests/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out docs/api",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "groq",
    "llm",
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "agent",
    "web-browsing",
    "embeddings",
    "vector-search"
  ],
  "author": "mithun50",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mithun50/groq-rag.git"
  },
  "bugs": {
    "url": "https://github.com/mithun50/groq-rag/issues"
  },
  "homepage": "https://github.com/mithun50/groq-rag#readme",
  "dependencies": {
    "cheerio": "^1.0.0",
    "groq-sdk": "^0.8.0",
    "node-fetch": "^3.3.2",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.11.0",
    "@types/turndown": "^5.0.4",
    "eslint": "^9.0.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
