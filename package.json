{
  "name": "groq-rag",
  "version": "0.1.5",
  "description": "Extended Groq TypeScript SDK with RAG, web browsing, and agent capabilities - 100% groq-sdk API compatible",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/ tests/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "benchmark": "npx tsx benchmarks/benchmark.ts",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish",
    "publish:github": "npm run build && sed -i 's/\"name\": \"groq-rag\"/\"name\": \"@mithun50\\/groq-rag\"/' package.json && npm publish --registry=https://npm.pkg.github.com; sed -i 's/\"name\": \"@mithun50\\/groq-rag\"/\"name\": \"groq-rag\"/' package.json",
    "publish:all": "./scripts/publish-all.sh"
  },
  "keywords": [
    "groq",
    "groq-sdk",
    "groq-typescript",
    "groq-api",
    "llm",
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "agent",
    "web-browsing",
    "embeddings",
    "vector-search",
    "llama",
    "mixtral",
    "chatbot"
  ],
  "author": "mithun50",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mithun50/groq-rag.git"
  },
  "bugs": {
    "url": "https://github.com/mithun50/groq-rag/issues"
  },
  "homepage": "https://github.com/mithun50/groq-rag#readme",
  "dependencies": {
    "cheerio": "^1.0.0",
    "groq-sdk": "^0.8.0",
    "node-fetch": "^3.3.2",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.11.0",
    "@types/turndown": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
